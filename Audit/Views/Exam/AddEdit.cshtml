<script>
  $( function() {
      $(".datepicker").datepicker({
          beforeShow: function () {
              setTimeout(function () {
                  $('.ui-datepicker').css('z-index', 99999999999999);
              }, 0);
          },
          changeMonth: true,
          changeYear: true,
          dateFormat: "yy/mm/dd",
          firstDay: 1,
          dayNamesMin: ["Ня", "Да", "Мя", "Лха", "Пү", "Ба", "Бя"],
          monthNamesShort: ["1-сар", "2-сар", "3-сар", "4-сар", "5-сар", "6-сар", "7-сар", "8-сар", "9-сар", "10-сар", "11-сар", "12-сар"],
          maxDate: "+0D",
          showOn: "button",
          buttonImage: "@Url.Content("~/Images/calendar.png")",
          buttonImageOnly: true,
          buttonText: "Select date"
      });

      $("#SaveBtn").hide();
      $("#PrevBtn").hide();
      $(".wizard li a").on("click", function (event) {
          if (event.target.id == "step1") {
              $("#PrevBtn").hide();
          }
          else {
              $("#PrevBtn").show();
          }
          if (event.target.id == "step6") {
              $("#NextBtn").hide();
          }
          else {
              $("#NextBtn").show();
          }
      });

      $('#NextBtn').click(function () {
          $('.wizard .active').parent().next('li').find('a').trigger('click');
      });

      $('#PrevBtn').click(function () {
          $('.wizard .active').parent().prev('li').find('a').trigger('click');
      });
  });
</script>