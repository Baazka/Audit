                       UpdateTargetId = "exammodalcontainer",
                       OnBegin = "Modal.show()",
                       OnComplete = "Modal.hide(); $('#ExamModal').modal({ show: true }); $('.modal-backdrop:first').remove()",
                       OnSuccess = "Exam.progressResponse(data, status, xhr)",
                       OnFailure = "errorResponse",
                       HttpMethod = "Post",
