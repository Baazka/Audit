</style>
<div class="modal fade" id="ExamModal" tabindex="-1" role="dialog">
    <div class="modal-dialog" role="document" style="max-width:90%;margin: 0 auto;">
        @using (Ajax.BeginForm("AddEdit", "Exam", new { Area = "" },
