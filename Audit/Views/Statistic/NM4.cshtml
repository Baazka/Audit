@model System.Xml.Linq.XElement
@{
    ViewBag.Title = "NM4";
    Layout = "~/Views/Shared/_Layout.cshtml";
    int counter = 0;
}
<div class="d-flex">
    <a href="@Url.Action("Index","Statistic")" class="btn btn-sm btn-light mr-2">
        <span class="icon text-gray-600">
            <i class="fas fa-arrow-left"></i>
        </span>
    </a>
    <h4>НМ-4. Төрийн аудитын газрын албан тушаалтанд хариуцлага тооцуулах саналын нэгтгэл</h4>
</div>
<hr class="sidebar-divider">
<div class="table-responsive">
    <table class="table table-bordered datatable display" style="width:100%">
        <thead>
            <tr style="background-color:#D2D3D5;">
                <th rowspan="3" class="text-center">№</th>
                <th rowspan="3">Аудитын код</th>
                <th rowspan="3">Аудитын нэр</th>
                <th rowspan="2" colspan="2">Хариуцлага тооцуулах санал</th>
                <th colspan="4">Албан тушаалтанд хариуцлага тооцуулах саналын хэрэгжилт</th>
            </tr>
            <tr>
                <th colspan="2">Бүрэн хэрэгжсэн</th>
                <th colspan="2">Хэрэгжээгүй</th>
            </tr>
            <tr>
                <th>Тоо</th>
                <th>Дүн</th>
                <th>Тоо</th>
                <th>Дүн</th>
                <th>Тоо</th>
                <th>Дүн</th>
            </tr>
        </thead>
        <tbody></tbody>
    </table>
</div>

@section scripts{
    <script>
        initDataTable();
    </script>
} 